        },
        "trayUnread": {
            "original": "media/IconTrayUnread.png",
            "theme": "messenger-notification"
        },
        "trayUnread@2x": {
            "original": "media/IconTrayUnread@2x.png",
            "theme": "messenger-notification"
        },
        "trayUnread@4x": {
            "original": "media/IconTrayUnread@4x.png",
            "theme": "messenger-notification"
