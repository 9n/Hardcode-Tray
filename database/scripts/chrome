#!/bin/bash
cd ./database/scripts/
if [[ $1 == *.svg ]]; then
    python3 svg2png.py $1 ./extracted/$(echo $2 | cut -f 1 -d '.')
else
    cp $1 ./extracted/$(echo $2 | cut -f 1 -d '.')
fi

./data_pack.py chrome_100_percent.pak $(echo $2 | cut -f 1 -d '.')
cp chrome_100_percent.pak $3/chrome_100_percent.pak
